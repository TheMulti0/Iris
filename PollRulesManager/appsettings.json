{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      }
    }
  },
  "MongoDb": {
    "ConnectionString": "mongodb://mongodb:27017",
    "DatabaseName": "UsersDb"
  },
  "ChatPollRequestsConsumer": {
    "ConnectionString": "amqp://guest:guest@rabbitmq:5672//",
    "Destination": "chatPollRequests"
  },
  "PollRequestsProducer": {
    "ConnectionString": "amqp://guest:guest@rabbitmq:5672//",
    "Destination": "pollRequests"
  }
}
