{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      }
    }
  },
  "SubscriptionsDb": {
    "ConnectionString": "mongodb://mongodb:27017",
    "DatabaseName": "SubscriptionsDb"
  },
  "RabbitMqConnection": {
    "ConnectionString": "amqp://guest:guest@rabbitmq:5672//"
  },
  "RabbitMqConsumer": {
    "Queue": "subscriptionRequests"
  },
  "RabbitMqProducer": {
    "Exchange": "pollRequests"
  }
}
