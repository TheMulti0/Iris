{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "SubscriptionsDb": {
        "ConnectionString": "mongodb://mongodb:27017",
        "DatabaseName": "SubscriptionsDb"
    },
    "RabbitMqConnection": {
        "ConnectionString": "amqp://guest:guest@rabbitmq:5672//"
    },
    "RabbitMqConsumer": {
        "AutoAcknowledge": false,
        "RejectRequeue": true
    },
    "RabbitMqProducer": {
        "Exchange": "messages"
    },
    "ScraperRabbitMqClient": {
        "ServerUri": "http://scraper.rabbitmq:5000"
    }
}