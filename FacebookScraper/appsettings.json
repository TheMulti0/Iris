{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "MongoDb": {
        "ConnectionString": "mongodb://mongodb:27017",
        "DatabaseName": "FacebookScraperDb",
        "SentUpdatesExpiration": "24:00:00"
    },
    "RabbitMqConnection": {
        "ConnectionString": "amqp://guest:guest@rabbitmq:5672//"
    },
    "RabbitMqConsumer": {
        "Queue": "facebookJobs"
    },
    "RabbitMqProducer": {
        "Exchange": "updates"
    },
    "Scraper": {
        "StoreSentUpdates": true
    },
    "UpdatesProvider": {
        "Name": "Facebook"
    }
}