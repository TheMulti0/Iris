{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "RabbitMqConnection": {
        "ConnectionString": "amqp://guest:guest@localhost:5672//"
    },
    "RabbitMqConsumer": {
        "Queue": "twitterJobs"
    },
    "RabbitMqProducer": {
        "Exchange": "updates"
    },
    "UpdatesProvider": {
        "ConsumerKey": "consumerkey",
        "ConsumerSecret": "consumersecret"
    },
    "UpdatesScraperDb": {
        "ConnectionString": "mongodb://localhost:27017/",
        "DatabaseName": "TwitterScraperDb"
    },
    "Scraper": {
        "StoreSentUpdates": false
    }
}