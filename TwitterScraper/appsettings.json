{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "MongoDb": {
        "ConnectionString": "mongodb://mongodb:27017",
        "DatabaseName": "TwitterScraperDb",
        "SentUpdatesExpiration": "24:00:00"
    },
    "UpdatesProducer": {
        "ConnectionString": "amqp://guest:guest@rabbitmq:5672//",
        "Destination": "updates"
    },
    "JobsConsumer": {
        "ConnectionString": "amqp://guest:guest@rabbitmq:5672//",
        "Destination": "twitterJobs"
    },
    "UpdatesProvider": {
        "Name": "Twitter",
        "ConsumerKey": "consumerkey",
        "ConsumerSecret": "consumersecret",
        "BearerToken": "bearertoken"
    },
    "Scraper": {
        "StoreSentUpdates": false
    },
    "VideoExtractor": {
        "FormatRequest": "best"
    }
}